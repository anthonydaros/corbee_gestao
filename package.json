{
	"name": "@devanthonymax/n8n-nodes-corbeegestao",
	"version": "2.0.0",
	"description": "n8n community node for Corbee GestÃ£o API integration - User, Broker and Proposal management",
	"keywords": [
		"n8n-community-node-package",
		"corbee",
		"gestao",
		"broker",
		"user-management",
		"proposal-management",
		"fgts",
		"api-integration"
	],
	"license": "MIT",
	"homepage": "https://github.com/anthonydaros/corbee_gestao",
	"author": {
		"name": "Anthony Daros",
		"email": "devanthonymax@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/anthonydaros/corbee_gestao.git"
	},
	"engines": {
		"node": ">=18.10",
		"pnpm": ">=8.6"
	},
	"packageManager": "pnpm@8.6.2",
	"main": "index.js",
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes/**/*.ts credentials/**/*.ts package.json",
